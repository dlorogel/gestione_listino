<mvc:View controllerName="it.orogel.gestionelistino.controller.ListaTrasp"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:m="sap.m"
    xmlns:f="sap.f"
    xmlns:c="sap.ui.core"
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
	xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:smartForm="sap.ui.comp.smartform" 
    xmlns:smartMultiInput="sap.ui.comp.smartmultiinput"  
    xmlns:layout="sap.ui.layout"
    xmlns:o="sap.ui.layout.form"
    xmlns="sap.ui.table"
    xmlns:u="sap.ui.unified"
    xmlns:fb="sap.ui.comp.filterbar">
	<f:DynamicPage id="dynamicPageId">
		<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<m:Title text="{i18n>list4}"/>
				</f:heading>
				<f:content>
						
					</f:content>
			</f:DynamicPageTitle>
		</f:title>
		<f:header>
			<f:DynamicPageHeader pinnable="true">
				<layout:HorizontalLayout allowWrapping="true">
					<layout:VerticalLayout class="sapUiMediumMarginEnd">
						</layout:VerticalLayout>
				</layout:HorizontalLayout>
				<f:content>
					<fb:FilterBar reset="onReset" search="onSearchFilter" showRestoreButton="true" showClearButton="true">
						<fb:filterGroupItems>
							<fb:FilterGroupItem groupName="__$INTERNAL$" name="A" label="{i18n>EKORG}" partOfCurrentVariant="true" visibleInFilterBar="true">
								<fb:control>
									<m:Input value="{filterModel>/EKORG}"/>
								</fb:control>
							</fb:FilterGroupItem>
							<fb:FilterGroupItem groupName="__$INTERNAL$" name="E" label="{i18n>ZDAT_FROM}" labelTooltip="Tooltip Example" partOfCurrentVariant="true" visibleInFilterBar="true">
								<fb:control>
									<m:DatePicker value="{filterModel>/ZDAT_FROM}" displayFormat="dd/MM/yyyy" valueFormat="yyyy-MM-dd"/>
								</fb:control>
							</fb:FilterGroupItem>
                            <fb:FilterGroupItem groupName="__$INTERNAL$" name="E1" label="{i18n>ZDAT_TO}" labelTooltip="Tooltip Example" partOfCurrentVariant="true" visibleInFilterBar="true">
								<fb:control>
									<m:DatePicker value="{filterModel>/ZDAT_TO}" displayFormat="dd/MM/yyyy" valueFormat="yyyy-MM-dd"/>
								</fb:control>
							</fb:FilterGroupItem>
							<fb:FilterGroupItem groupName="__$INTERNAL$" name="B" label="{i18n>ZSPECIE}" labelTooltip="Tooltip Example" partOfCurrentVariant="true" visibleInFilterBar="true">
								<fb:control>
									<m:Input value="{filterModel>/ZSPECIE}"/>
								</fb:control>
							</fb:FilterGroupItem>
                            <fb:FilterGroupItem groupName="__$INTERNAL$" name="U" label="{i18n>ZINCO1}" labelTooltip="Tooltip Example" partOfCurrentVariant="true" visibleInFilterBar="true">
								<fb:control>
									<m:Input value="{filterModel>/ZINCO1}"/>
								</fb:control>
							</fb:FilterGroupItem>
                            <fb:FilterGroupItem groupName="__$INTERNAL$" name="Z" label="{i18n>ZZONATRA}" labelTooltip="Tooltip Example" partOfCurrentVariant="true" visibleInFilterBar="true">
								<fb:control>
									<m:Input value="{filterModel>/ZZONATRA}"/>
								</fb:control>
							</fb:FilterGroupItem>
                            <fb:FilterGroupItem groupName="__$INTERNAL$" name="X" label="{i18n>LGORT}" labelTooltip="Tooltip Example" partOfCurrentVariant="true" visibleInFilterBar="true">
								<fb:control>
									<m:Input value="{filterModel>/LGORT}"/>
								</fb:control>
							</fb:FilterGroupItem>
						</fb:filterGroupItems>
					</fb:FilterBar>
				</f:content>
			</f:DynamicPageHeader>
		</f:header>
		<f:content>
			<m:ScrollContainer id="dynheader" vertical="true" horizontal="false">
				<!--<m:Input id="productInput2" type="{varianteModel>/variante}" placeholder="{i18n>phVariant}" value="{selectedVarModel>/selected/variante}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest="onOpenPopover" width="auto"/>-->
				<layout:HorizontalLayout class="sapUiContentPadding">
                <m:Button text="{i18n>btnInsert}" type="Emphasized" press=""/>
                <m:Button text="{i18n>btnModify}" type="Emphasized" press=""/>
                <m:Button text="{i18n>btnDelete}" type="Emphasized" press=""/>
                <m:Button text="{i18n>btnRestore}" type="Emphasized" press=""/>
                <m:Button text="{i18n>btnImport}" type="Emphasized" press=""/>
                <m:Button text="{i18n>btnExport}" type="Emphasized" press=""/>
                <m:Button text="{i18n>btnApplyIncrement}" type="Emphasized" press=""/>
             
		
	</layout:HorizontalLayout>
				<Table id="listaTrasp" rows="{path: '/ZZ1_ZLISTINO_TRASP',
                            parameters: {filter: {path: 'EKORG;ZTIPLIST;ZDAT_FROM;ZDAT_TO;ZSPECIE;ZSTAG;ZVARIETA;ZGRVARIETA;ZCERTAZ;ZCERTPROD;ZRESIDUO;ZCERTIFCOM;ZSPECIFICA;ZLAVORAZIONE;ZORIGINE;ZQUALITA;ZCALIBRAZIONE;ZRAGGCAL;ZPRZBASE;ZUM;ZEVCOLT;ZSERRACTAG;ZSERASTC;ZCOSSERASTC;ZSERCAR;ZCOSSERCAR;ZSERASSTEC;ZCOSSERASSTEC;ZSERDEP;ZCOSSERDEP;ZSERCAL;ZCOSSERCAL;ZSER1;ZCOSSER1;ZSER2;ZCOSSER2;ZSER3;ZCOSSER3;ZSER4;ZCOSSER4;ZSER5;ZCOSSER5;ZRAGGLIQ;ZOP;ZMARKUP;MATNR;ZCANC'}}}" selectionMode="MultiToggle" selectionBehavior="Row" rowSelectionChange=".onSelRow" alternateRowColors="true" visibleRowCount="15" paste="onPaste" ariaLabelledBy="title">
					<extension>
						<m:OverflowToolbar style="Clear">
							<m:Title id="title" text="{i18n>flpTitle}"/>
						</m:OverflowToolbar>
					</extension>
					<columns>
						<Column width="10em" sortProperty="EKORG" filterProperty="EKORG">
							<m:Label text="{i18n>EKORG}"/>
							<template>
								<m:Text text="{EKORG}" wrapping="false"/>
							</template>
						</Column>
						<Column width="10em" sortProperty="ZDAT_FROM" filterProperty="ZDAT_FROM">
							<m:Label text="{i18n>ZDAT_FROM}"/>
							<template>
								<m:Text text="{ZDAT_FROM}" wrapping="false"/>
							</template>
						</Column>
                        <Column width="10em" sortProperty="ZDAT_TO" filterProperty="ZDAT_TO">
							<m:Label text="{i18n>ZDAT_TO}"/>
							<template>
								<m:Text text="{ZDAT_TO}" wrapping="false"/>
							</template>
						</Column>
						<Column width="10em" sortProperty="ZSPECIE" filterProperty="ZSPECIE">
							<m:Label text="{i18n>ZSPECIE}"/>
							<template>
								<m:Text text="{ZSPECIE}" wrapping="false"/>
							</template>
						</Column>
						<Column width="10em" sortProperty="ZINCO1" filterProperty="ZINCO1">
							<m:Label text="{i18n>ZINCO1}"/>
							<template>
								<m:Text text="{ZINCO1}" wrapping="false"/>
							</template>
						</Column>
						<Column width="10em" sortProperty="ZZONATRA" filterProperty="ZZONATRA">
							<m:Label text="{i18n>ZZONATRA}"/>
							<template>
								<m:Text text="{ZZONATRA}" wrapping="false"/>
							</template>
						</Column>
						<Column width="10em" sortProperty="LGORT" filterProperty="LGORT">
							<m:Label text="{i18n>LGORT}"/>
							<template>
								<m:Text text="{LGORT}" wrapping="false"/>
							</template>
						</Column>
                        <Column width="10em" sortProperty="ZPRTRASP" filterProperty="ZPRTRASP">
							<m:Label text="{i18n>ZPRTRASP}"/>
							<template>
								<m:Text text="{ZPRTRASP}" wrapping="false"/>
							</template>
						</Column>
						<Column width="10em" sortProperty="ZCANC" filterProperty="ZCANC">
							<m:Label text="{i18n>ZCANC}"/>
							<template>
								<m:Text text="{ZCANC}" wrapping="false"/>
							</template>
						</Column>
					</columns>
				</Table>
			</m:ScrollContainer>
		</f:content>
	</f:DynamicPage>
